<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="273.0" prefWidth="464.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.library.btl_oop16_library.addUserDialogController">
   <opaqueInsets>
      <Insets top="10.0" />
   </opaqueInsets>
   <children>
      <VBox layoutY="23.0" prefHeight="287.0" prefWidth="464.0">
         <children>
            <TextField fx:id="nameField" layoutX="10.0" layoutY="10.0" maxWidth="385.0" prefHeight="25.0" promptText="name">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="emailFeild" maxWidth="385.0" prefHeight="25.0" promptText="email">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="accountField" maxWidth="385.0" prefHeight="25.0" promptText="account">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="passwordField" layoutX="10.0" layoutY="36.0" maxWidth="385.0" prefHeight="25.0" promptText="password">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <HBox alignment="CENTER_RIGHT" prefHeight="59.0" prefWidth="410.0">
               <children>
                  <Button fx:id="cancelButton" alignment="CENTER" mnemonicParsing="false" onAction="#onCancelButtonClick" prefHeight="34.0" prefWidth="110.0" text="Cancel" />
                  <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#onConfirmButtonClick" prefHeight="34.0" prefWidth="110.0" text="Confirm ">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets left="30.0" right="30.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
